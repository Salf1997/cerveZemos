<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>CerveZemos</name>
    <description>
        CerveZemos: una aplicacion para buscar bares y bebidas.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        CerveZemos
    </author>
    <content src="index.html" />
    <icon src="www/assets/images/ztransparente.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/assets/images/ztransparente.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/assets/images/ztransparente.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/assets/images/ztransparente.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/assets/images/ztransparente.png" />
    <icon gap:platform="winphone" src="www/assets/images/ztransparente.png" />
    <icon gap:platform="winphone" gap:role="background" src="www/assets/images/ztransparente.png" />
    <access origin="*" />
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="android-installLocation" value="auto" />
</widget>
